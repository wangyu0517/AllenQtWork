<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!--##########################################################################-->
	<!--##########################终端AV组件与音视频组件##########################-->
	<!--##########################################################################-->
	<!--客户端登录请求-->
	<cmd id="8701" ver="0">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--用户ID-->
		<FrontUserID val="1" /> 
		<!--校验码-->
		<CheckCode val="123456"/>
	</cmd> 

	<!--服务器应答登录请求-->
	<cmd id="8702" ver="0" >
		<!--返回结果-->
		<Result val="0"/>
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--用户ID-->
		<FrontUserID val="1" /> 
		<!--合成用户ID-->
		<MixerUserID val="9999" /> 
	</cmd>

	<!--客户端请求接收或停止接收音视频-->
	<cmd id="8703">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否接收，0-停止接收，1-接收-->
		<Recv val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--用户ID-->
		<FrontUserID val="1" /> 
		<!--数据源用户ID-->
		<SrcUserID val="2" /> 
	</cmd>
	
	<!--服务器应答接收或停止音视频请求-->
	<cmd id="8704">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否接收，0-停止接收，1-接收-->
		<Recv val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--返回结果-->
		<Result val="0"/>
		<!--用于数据传输的通道号ID-->
		<ChannelID val="1"/>
		<!--数据传输通道验证码-->
		<ChannelCheckCode val="123456"/>
		<!--用户ID-->
		<FrontUserID val="1" /> 
		<!--数据源用户ID-->
		<SrcUserID val="2" /> 
	</cmd>
	
	<!--服务器通知发送端发送或停止发送数据-->
	<cmd id="8705">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否发送，0-停止发送，1-发送-->
		<Send val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--用于数据传输的通道号ID-->
		<ChannelID val="1"/>
		<!--数据传输通道验证码-->
		<ChannelCheckCode val="123456"/>
		<!--用户ID-->
		<FrontUserID val="2" /> 
	</cmd>
	
	<!--客户端应答服务器数据发送通知-->
	<cmd id="8706">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否发送，0-停止发送，1-发送-->
		<Send val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--返回结果-->
		<Result val="0"/>
		<!--用户ID-->
		<FrontUserID val="2" /> 
	</cmd>
	
	<!--客户端请求暂停或恢复接收用户音视频-->
	<cmd id="8707">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否暂停接收，0-恢复接收，1-暂停-->
		<Pause val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--用户ID-->
		<FrontUserID val="1" />
		<!--数据源用户ID-->
		<SrcUserID val="2" /> 		
	</cmd>
	
	<!--服务器应答客户端暂停接收用户音视频请求-->
	<cmd id="8708">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--是否暂停接收，0-恢复接收，1-暂停-->
		<Pause val="0"/>
		<!--媒体类型，1-音频数据，2-视频数据-->
		<MediaType val="1" />
		<!--媒体ID-->
		<MediaID val="1" />
		<!--返回结果-->
		<Result val="0"/>
		<!--用户ID-->
		<FrontUserID val="1" />
		<!--数据源用户ID-->
		<SrcUserID val="2" /> 		
	</cmd>
	
	<!--关闭信令通道-->
	<cmd id="8709">
		<!--房间的GUID-->
		<Guid var="{CAF614F2-1230-4bff-960D-926B52968E31}" />
		<!--用户ID-->
		<FrontUserID val="1" />
	</cmd>	
</root>